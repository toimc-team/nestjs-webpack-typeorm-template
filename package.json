{
  "name": "nestjs-template",
  "version": "1.0.0",
  "description": "nestjs project: hotModule,lint-staged,jwt-auth,logger,mongodb-TypeOrm,cofig",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=development nest build --watch --webpack --webpackPath webpack-hmr.config.js",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint:eslint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md,ts}\" \"test/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs -d ormconfig.ts",
    "migration:generate": "f() { npm run typeorm migration:generate -p \"./src/migrations/$@\"; }; f",
    "migration:create": "typeorm-ts-node-commonjs migration:create",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "log": "standard-version"
  },
  "dependencies": {
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^9.3.9",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/terminus": "^9.2.1",
    "@nestjs/typeorm": "9.0.1",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "config": "^3.3.9",
    "dotenv": "^16.0.3",
    "express-rate-limit": "^6.7.0",
    "fast-glob": "^3.2.12",
    "helmet": "^7.0.0",
    "mongodb": "^3.7.3",
    "mysql2": "^3.2.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.4.0",
    "rxjs": "^7.8.0",
    "swagger-ui-express": "^4.6.2",
    "typeorm": "^0.3.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.9",
    "@types/express": "^4.17.17",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "29.5.2",
    "@types/node": "^18.15.0",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.12",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "run-script-webpack-plugin": "^0.2.0",
    "standard-version": "^9.5.0",
    "start-server-webpack-plugin": "^2.2.5",
    "supertest": "^6.3.3",
    "terser-webpack-plugin": "^5.3.7",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "^4.9.5",
    "webpack": "^5.76.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }
}